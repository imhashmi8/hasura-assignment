            },
            "object_relationships": [
              {
                "name": "track_album",
                "using": {
                  "foreign_key_constraint_on": "album_id"
                }
              },
              {
                "name": "track_genre",
                "using": {
                  "foreign_key_constraint_on": "genre_id"
                }
              }
            ],
            "select_permissions": [
              {
                "role": "artist",
                "permission": {
                  "columns": [
                    "unit_price"
                  ],
                  "filter": {
                    "track_album": {
                      "artist_id": {
                        "_eq": "x-hasura-artist-id"
                      }
                    }
                  },
                  "allow_aggregations": true
                }
              }
            ]
          }
        ],
        "configuration": {
