{
  "resource_version": 38,
  "metadata": {
    "version": 3,
    "sources": [
      {
        "name": "chinook",
        "kind": "postgres",
        "tables": [
          {
            "table": {
              "name": "album",
              "schema": "public"
            },
            "array_relationships": [
              {
                "name": "album_tracks",
                "using": {
                  "foreign_key_constraint_on": {
                    "column": "album_id",
                    "table": {
                      "name": "track",
                      "schema": "public"
                    }
                  }
                }
              }
            ],
            "select_permissions": [
              {
                "role": "artist",
                "permission": {
                  "columns": [
                    "album_id",
                    "title"
                  ],
                  "filter": {
                    "artist_id": {
                      "_eq": "x-hasura-artist-id"
                    }
                  }
                }
              }
            ]
          },
          {
            "table": {
              "name": "artist",
              "schema": "public"
            }
